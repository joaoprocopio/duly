{
  "$schema": "http://json.schemastore.org/prettierrc",
  "vueIndentScriptAndStyle": true,
  "printWidth": 120,
  "quoteProps": "consistent",
  "bracketSameLine": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "singleQuote": false,
  "semi": false,
  "trailingComma": "none",
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false
}
