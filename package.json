{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-path .gitignore",
    "check": "prettier . --check --ignore-path .gitignore",
    "format": "npm run check -- --write && npm run lint -- --fix",
    "typecheck": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@rushstack/eslint-config": "^3.5.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
